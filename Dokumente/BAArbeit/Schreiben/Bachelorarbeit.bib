@misc{nashtara_islam_z8_2006,
	title = {Z8 {Encore} {Microcontroller} {Based} {Data} {Logging} {System}},
	abstract = {Data logging systems find numerous important applications in fields ranging from
sport science to automotive engineering. The need for accurate, robust and low-cost
data logging solutions has become obvious as machine, process and product
inspections are becoming critical issues. Various data logging systems have been
developed by manufacturers around the globe but they are neither flexible nor cost-
effective for the end user. In this paper, the design and construction of a novel Z8
Encore based embedded data logging system is discussed in detail. A GPS device is
used to provide the timing information of the logged data. This particular data logger
provides the user with the desired accuracy, flexibility and cost-effectiveness rarely
seen in commercially available devices. Complete discussions and various test results
are presented to evaluate the performance of the system.},
	author = {{Nashtara Islam} and {Robert Watson} and {Philip Moore}},
	month = oct,
